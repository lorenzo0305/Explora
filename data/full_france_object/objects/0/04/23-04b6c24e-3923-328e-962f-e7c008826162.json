  "rdfs:comment": {
    "de": [
      "Lamm-Mechoui im Relais de la Franval. Vor Ort Musik, Boulespielplatz, bretonische Shuffleboards, Cornhole, Kegeln, Kartenspiele...\nMahlzeiten auf Vorbestellung."
    ],
    "en": [
      "Lamb barbecue at Relais de la Franval. On-site music, pétanque court, Breton shuffleboard, cornhole, skittles, card games...\nMeals on reservation."
    ],
    "it": [
      "Grigliata di agnello al Relais de la Franval. Musica in loco, campo da bocce, shuffleboard bretone, cornhole, birilli, giochi di carte...\nPasti su prenotazione."
    ],
    "fr": [
      "Méchoui d'agneau au Relais de la Franval. Sur place musique, terrain de pétanque, jeux de palets bretons, cornhole, quilles, jeux de cartes... \nRepas sur réservation."
    ],
    "nl": [
      "Lamsbarbecue in het Relais de la Franval. Muziek ter plaatse, petanqueterrein, Bretonse sjoelbak, cornhole, kegelspel, kaartspelen...\nMaaltijden op reservering."
    ],
    "es": [
      "Barbacoa de cordero en el Relais de la Franval. Música en el lugar, zona de petanca, tejo bretón, cornhole, bolos, juegos de cartas...\nComidas previa reserva."
    ]
  },
