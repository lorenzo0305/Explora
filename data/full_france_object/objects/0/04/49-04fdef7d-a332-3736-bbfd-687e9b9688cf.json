  "rdfs:comment": {
    "de": [
      "Begleitete Spaziergänge und typische Entdeckungen an den Ufern der Loire und des Flusses Layon."
    ],
    "en": [
      "Guided walks and typical discoveries of the banks of the Loire and the Layon."
    ],
    "it": [
      "Passeggiate guidate e scoperte tipiche lungo le rive dei fiumi Loira e Layon."
    ],
    "fr": [
      "Balades accompagnées et découvertes typiques des bords de Loire et du Layon."
    ],
    "es": [
      "Paseos guiados y descubrimientos típicos a orillas de los ríos Loira y Layón."
    ],
    "nl": [
      "Begeleide wandelingen en typische ontdekkingen langs de oevers van de Loire en de Layon."
    ]
  },
