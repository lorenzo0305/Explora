  "rdfs:comment": {
    "de": [
      "Um 20:30 Uhr - Pétanque-Platz (neben dem Tennisplatz). Pétanque-Wettbewerb in Doubletten mit 3 Partien, organisiert vom Pétan' Club de Salles-Curan. Anmeldungen vor Ort, offen für alle."
    ],
    "en": [
      "8:30pm - Petanque court (next to the Tennis court). Pétan' Club de Salles-Curan doubles pétanque competition, 3 games. Registration on site, open to all."
    ],
    "it": [
      "20.30 - Campo da petanque (accanto al campo da tennis). Il Pétan' Club de Salles-Curan organizza una gara di bocce in doppio con 3 partite. Iscrizione sul posto, aperta a tutti."
    ],
    "fr": [
      "A 20h30 - Terrain de pétanque (à côté du terrain de Tennis). Concours de pétanque en doublettes en 3 parties organisé par le Pétan' Club de Salles-Curan. Inscriptions sur place, ouvert à tous."
    ],
    "es": [
      "20.30 h - Campo de petanca (junto a la pista de tenis). Pétan' Club de Salles-Curan organiza una competición de petanca de dobles a 3 partidas. Inscripción in situ, abierta a todos."
    ],
    "nl": [
      "20.30 - Petanqueterrein (naast de tennisbaan). Pétan' Club de Salles-Curan organiseert een petanque-dubbelcompetitie met 3 wedstrijden. Inschrijving ter plaatse, open voor iedereen."
    ]
  },
