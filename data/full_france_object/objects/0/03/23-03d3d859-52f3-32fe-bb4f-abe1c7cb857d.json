  "rdfs:comment": {
    "de": [
      "Pizzeria mit Holzfeuer, Bierbar. + Tabak, FDJ, PMU. Terrasse.\nZum Mitnehmen oder vor Ort, in der Sommersaison täglich von 11:30/14:00 und 18:00/22:00 Uhr.\nBar täglich von 7 Uhr bis 22 Uhr geöffnet."
    ],
    "en": [
      "Wood-fired pizzeria, beer bar + Tabac, FDJ, PMU. Terrace.\nTakeaway or on site, daily during the summer season from 11h30/14h and 18h/22h.\nBar open daily from 7am to 10pm."
    ],
    "it": [
      "Pizzeria a legna, birreria + tabaccheria, FDJ, PMU. Terrazza.\nDa asporto o in loco tutti i giorni durante la stagione estiva dalle 11.30 alle 14.00 e dalle 18.00 alle 22.00.\nBar aperto tutti i giorni dalle 7.00 alle 22.00."
    ],
    "fr": [
      "Pizzeria au feu de bois, bar à bières. + Tabac, FDJ, PMU. Terrasse.\nA emporter ou sur place, tous les jours pour la saison estivale de  11h30/14h et de 18h/22h.\nBar ouvert tous les jours de 7h à 22h."
    ],
    "es": [
      "Pizzería de leña, cervecería + estanco, FDJ, PMU. Terraza.\nComida para llevar o in situ todos los días durante la temporada de verano de 11.30 a 14.00 h y de 18.00 a 22.00 h.\nBar abierto todos los días de 7.00 a 22.00 h."
    ],
    "nl": [
      "Houtgestookte pizzeria, bierbar + tabakswinkel, FDJ, PMU. Terras.\nAfhaalmaaltijden of ter plaatse tijdens het zomerseizoen elke dag van 11.30 tot 14.00 en van 18.00 tot 22.00.\nBar dagelijks geopend van 7.00 tot 22.00 uur."
    ]
  },
