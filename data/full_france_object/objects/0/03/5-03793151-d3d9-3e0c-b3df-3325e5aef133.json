  "rdfs:comment": {
    "de": [
      "Duplex-Wohnung von 100m². 2 Schlafzimmer, 2 Badezimmer. Ausgestattete Küche. Kostenloser Parkplatz in 200m Entfernung. Ideale Lage im Stadtzentrum. Komplett neu renoviert mit Blick auf die Dächer, wird es Ihnen einen unvergesslichen Aufenthalt bereiten."
    ],
    "en": [
      "100m² duplex apartment. 2 bedrooms, 2 bathrooms. Fully equipped kitchen. Free parking 200m away. Ideally located downtown. Completely refurbished with rooftop views, you'll enjoy an unforgettable stay."
    ],
    "it": [
      "Appartamento duplex di 100 m². 2 camere da letto, 2 bagni. Cucina attrezzata. Parcheggio gratuito a 200 m di distanza. Situato in posizione ideale nel centro della città. Completamente ristrutturato con vista sui tetti, renderà il vostro soggiorno indimenticabile."
    ],
    "fr": [
      "Appartement duplex de 100m². 2 chambres, 2 sdb. Cuisine équipée. Parking gratuit à 200m. Idéalement situé en centre-ville. Entièrement refait à neuf avec vue sur les toits, il vous fera passer un séjour inoubliable."
    ],
    "es": [
      "Piso dúplex de 100m². 2 dormitorios, 2 baños. Cocina amueblada. Parking gratuito a 200m. Idealmente situado en el centro de la ciudad. Completamente reformado y con vistas a los tejados, hará que su estancia sea inolvidable."
    ],
    "nl": [
      "Duplexwoning van 100m². 2 slaapkamers, 2 badkamers. Ingerichte keuken. Gratis parking op 200m. Ideaal gelegen in het centrum van de stad. Volledig gerenoveerd met uitzicht over de daken, zal het uw verblijf onvergetelijk maken."
    ]
  },
