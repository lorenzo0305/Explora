  "rdfs:comment": {
    "de": [
      "Eine Stunde von Toulouse entfernt befindet sich der Bois Perché am Fuße der Zentralpyrenäen in der Nähe des unteren Pyrenäenwaldes in Aspet!"
    ],
    "en": [
      "Just an hour from Toulouse, Le Bois Perché is located at the foot of the Central Pyrenees, close to the Pyrenean forest at Aspet!"
    ],
    "it": [
      "A un'ora da Tolosa, Le Bois Perché si trova ai piedi dei Pirenei centrali, vicino alla foresta dei Pirenei inferiori di Aspet!"
    ],
    "fr": [
      "A une heure de Toulouse, le Bois Perché se trouve au pied des Pyrénées Centrales à proximité de la basse forêt pyrénéenne à Aspet !"
    ],
    "nl": [
      "Op slechts een uur van Toulouse ligt Le Bois Perché aan de voet van de Centrale Pyreneeën, dicht bij het bos van Aspet!"
    ],
    "es": [
      "A sólo una hora de Toulouse, Le Bois Perché se encuentra a los pies de los Pirineos Centrales, cerca del bosque del Bajo Pirineo de Aspet"
    ]
  },
