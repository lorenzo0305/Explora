  "rdfs:comment": {
    "de": [
      "Restaurant, Bar, Brasserie im Stadtzentrum von Saint-Palais-sur-Mer. Unumgängliche Institution in Saint-Palais, um morgens einen Kaffee zu trinken. Große Auswahl an Softdrinks, Cocktails und ausgezeichneten Wurstplatten Eis."
    ],
    "en": [
      "Restaurant, bar, brasserie located in the city center of Saint-Palais-sur-Mer. A must in Saint Palais to have a coffee in the morning. Large choice of soft drinks, cocktails and platters of excellent ice cream."
    ],
    "it": [
      "Ristorante, bar, brasserie situato nel centro di Saint-Palais-sur-Mer. Un must a Saint Palais per prendere un caffè al mattino. Ampia scelta di bevande analcoliche, cocktail e vassoi di ottimo gelato."
    ],
    "fr": [
      "Restaurant, bar, brasserie situé dans le centre-ville de Saint-Palais-sur-Mer. Institution incontournable de Saint Palais pour prendre un café le matin. Grand choix de boissons soft, cocktails et planches de charcuterie excellentes glaces."
    ],
    "es": [
      "Restaurante, bar y brasserie situados en el centro de Saint-Palais-sur-Mer. Una visita obligada en Saint Palais para tomar un café por la mañana. Gran variedad de refrescos, cócteles y bandejas de excelentes helados."
    ],
    "nl": [
      "Restaurant, bar, brasserie gelegen in het centrum van Saint-Palais-sur-Mer. Een must in Saint Palais om 's morgens een koffie te drinken. Grote keuze aan frisdranken, cocktails en schalen met uitstekend ijs."
    ]
  },
