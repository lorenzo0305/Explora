  "rdfs:comment": {
    "de": [
      "Der KLEINE See von Belle-Isle liegt ideal im Stadtzentrum von Châteauroux und ist ein Anziehungspunkt für passionierte Angler."
    ],
    "en": [
      "Ideally located in the heart of Châteauroux, the PETIT lac de Belle-Isle attracts fishing enthusiasts."
    ],
    "it": [
      "Situato in posizione ideale nel cuore di Châteauroux, il piccolo lago Belle-Isle attira gli appassionati di pesca."
    ],
    "fr": [
      "Idéalement situé en cœur de ville de Châteauroux, le PETIT lac de Belle-Isle attire les passionnés de pêche."
    ],
    "es": [
      "Idealmente situado en el corazón de Châteauroux, el PEQUEÑO lago de Belle-Isle atrae a los aficionados a la pesca."
    ],
    "nl": [
      "Het kleine meer van Belle-Isle, ideaal gelegen in het hart van Châteauroux, trekt visliefhebbers."
    ]
