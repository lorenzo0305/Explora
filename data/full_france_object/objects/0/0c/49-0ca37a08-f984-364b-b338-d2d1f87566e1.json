  "rdfs:comment": {
    "de": [
      "Willkommen in Le Chêne Vert in Savenay. Unser Hotel/Restaurant empfängt Sie das ganze Jahr über in einer familiären und herzlichen Atmosphäre, mit Freunden, Kollegen oder der Familie."
    ],
    "en": [
      "Welcome to Le Chêne Vert in Savenay, our Hotel/Restaurant welcomes you all year round in a warm and friendly atmosphere, with friends, colleagues or family."
    ],
    "it": [
      "Benvenuti allo Chêne Vert di Savenay, il nostro hotel/ristorante vi accoglie tutto l'anno in una calda atmosfera familiare, con gli amici, i colleghi o la famiglia."
    ],
    "fr": [
      "Bienvenue au Chêne Vert à Savenay, notre Hôtel/Restaurant vous accueille tout au long de l'année dans une ambiance familiale et chaleureuse, entre amis, collègues ou en famille."
    ],
    "nl": [
      "Welkom in de Chêne Vert in Savenay, ons hotel/restaurant verwelkomt u het hele jaar door in een warme familiesfeer, met vrienden, collega's of familie."
    ],
    "es": [
      "Bienvenido al Chêne Vert en Savenay, nuestro hotel/restaurante le da la bienvenida todo el año en un cálido ambiente familiar, con amigos, colegas o familia."
    ]
  },
