  "rdfs:comment": {
    "de": [
      "Entdecken Sie den Espace luge Front de neige de la Ramasse in Val-Cenis! Ideal für Kinder und Anfänger, erleben Sie Momente puren Vergnügens auf dem Schnee. Lachen mit der ganzen Familie ist garantiert!"
    ],
    "en": [
      "Discover the Espace luge Front de neige de la Ramasse in Val-Cenis! Ideal for children and beginners, experience pure fun on the snow. Fun for all the family!"
    ],
    "it": [
      "Scoprite l'Espace luge Front de neige de la Ramasse in Val-Cenis! Ideale per bambini e principianti, per un divertimento puro sulla neve. Divertimento per tutta la famiglia!"
    ],
    "fr": [
      "Découvrez l'Espace luge Front de neige de la Ramasse à Val-Cenis ! Idéal pour les enfants et débutants, vivez des moments de pur plaisir sur la neige. Fous rires garantis en famille !"
    ],
    "es": [
      "¡Descubre el Espace luge Front de neige de la Ramasse en Val-Cenis! Ideal para niños y principiantes, disfrute de pura diversión en la nieve. ¡Diversión para toda la familia!"
    ],
    "nl": [
      "Ontdek de Espace luge Front de neige de la Ramasse in Val-Cenis! Ideaal voor kinderen en beginners, puur plezier in de sneeuw. Plezier voor het hele gezin!"
    ]
  },
