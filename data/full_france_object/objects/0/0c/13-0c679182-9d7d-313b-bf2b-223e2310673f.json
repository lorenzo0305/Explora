  "rdfs:comment": {
    "de": [
      "Dorf Saint-Vert: Priorat mit Elementen aus dem 14. Ehemaliges befestigtes Haus aus dem 14. bis 15. Jahrhundert mit Sprossenfenstern und Türmchen. Croix du Muletier nach Peymian."
    ],
    "en": [
      "Village of Saint-Vert: Priory with 14th-century features. Former 14th-15th-century fortified house with mullions and turrets. Muletier cross after Peymian."
    ],
    "it": [
      "Villaggio di Saint-Vert: Priorato con caratteristiche del XIV secolo. Ex casa fortificata del XIV-XV secolo con bifore e torrette. Croce del Mulattiere dopo Peymian."
    ],
    "fr": [
      "Village de Saint-Vert : Prieuré avec éléments du XIVe siècle. Ancienne maison forte des XIVe-XVe siècles avec meneaux et tourelles. Croix du Muletier après Peymian."
    ],
    "es": [
      "Pueblo de Saint-Vert: Priorato con elementos del siglo XIV. Antigua casa fortificada de los siglos XIV-XV con parteluces y torrecillas. Cruz del Arriero después de Peymian."
    ],
    "nl": [
      "Dorp Saint-Vert: Priorij met elementen uit de 14e eeuw. Voormalig versterkte huis uit de 14e-15e eeuw met muurschilden en torentjes. Kruis van de Muleteer naar Peymian."
    ]
  },
  "rdfs:label": {
