  "rdfs:comment": {
    "de": [
      "Konzert Mr LEU & the Ackees\nMr LEU und seine sanften Ackees enthüllen Ihnen das festliche Jamaika, vom ursprünglichen Mento und seinen Kindern Ski und Reggae bis hin zum Calypso!"
    ],
    "en": [
      "Concert Mr LEU & the Ackees\nMr LEU and his sweet Ackees reveal the festive Jamaica, from the original Mento and its Ski and Reggae children to Calypso!"
    ],
    "it": [
      "Concerto Mr LEU e gli Ackees\nMr LEU e i suoi dolci Ackees svelano la Giamaica in festa, dal Mento originale e i suoi figli Ski e Reggae al Calypso!"
    ],
    "fr": [
      "Concert Mr LEU & the Ackees\nMr LEU et ses douces Ackees vous révèlent la festive Jamaïque, de l'originel Mento et ses enfants Ski et Reggae en swiguant vers le Calypso !"
    ],
    "es": [
      "Concierto Mr LEU & the Ackees\nMr LEU y sus dulces Ackees nos descubren la festiva Jamaica, ¡desde el Mento original y sus hijos Ski y Reggae hasta el Calypso!"
    ],
    "nl": [
      "Concert Mr LEU & de Ackees\nMr LEU en zijn lieve Ackees onthullen het feestelijke Jamaica, van de originele Mento en zijn Ski- en Reggae-kinderen tot Calypso!"
    ]
  },
