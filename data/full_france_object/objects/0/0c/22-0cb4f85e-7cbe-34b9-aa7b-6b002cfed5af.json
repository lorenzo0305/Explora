  "rdfs:comment": {
    "de": [
      "Fleurs Sauvages ist eine akustische Folk- und Popgruppe, die emblematische französische und englischsprachige Titel von den 50er Jahren bis heute in einer sanften und energiegeladenen Atmosphäre neu interpretiert."
    ],
    "en": [
      "Fleurs Sauvages is an acoustic folk and pop group that revisits iconic French and English songs from the 50s to today in a gentle, energetic ambience."
    ],
    "it": [
      "Fleurs Sauvages è un gruppo folk e pop acustico che rivisita canzoni iconiche francesi e inglesi dagli anni '50 a oggi in un ambiente dolce ed energico."
    ],
    "fr": [
      "Fleurs Sauvages est un groupe acoustique folk et pop qui revisite des titres emblématiques français et anglophones des années 50 à aujourd’hui dans une ambiance douce et énergique."
    ],
    "nl": [
      "Fleurs Sauvages is een akoestische folk- en popgroep die iconische Franse en Engelse liedjes uit de jaren '50 tot nu herneemt in een zachte, energieke sfeer."
    ],
    "es": [
      "Fleurs Sauvages es un grupo de folk y pop acústico que revisita canciones icónicas francesas e inglesas de los años 50 hasta nuestros días en un ambiente suave y enérgico."
    ]
  },
