  "rdfs:comment": {
    "de": [
      "Das Bistrot Populaire liegt gegenüber den Arenen und bietet Ihnen eine traditionelle Küche mit Gerichten aus Land und Meer."
    ],
    "en": [
      "Located opposite the Arena, the Bistrot Populaire offers traditional cuisine with land and sea dishes."
    ],
    "it": [
      "Situato di fronte all'Arena, il Bistrot Populaire offre una cucina tradizionale con piatti di terra e di mare."
    ],
    "fr": [
      "Situé face aux Arènes, le Bistrot Populaire vous propose une cuisine traditionnelle avec des plats de terre et de mer."
    ],
    "es": [
      "Situado frente al Arena, el Bistrot Populaire ofrece cocina tradicional con platos de la tierra y del mar."
    ],
    "nl": [
      "De Bistrot Populaire ligt tegenover de Arena en biedt een traditionele keuken met gerechten van het land en de zee."
    ]
  },
