  "rdfs:comment": {
    "de": [
      "Englisches Teehaus mit Backwaren, die man vor Ort essen oder mitnehmen kann. Außerdem gibt es jeden Mittag ein Tagesgericht. Ein kleines bisschen Großbritannien in der Drôme."
    ],
    "en": [
      "English Tea Room selling cakes to eat in and take away. A little bit of Britain in the Drôme."
    ],
    "it": [
      "Sala da tè inglese con dolci da mangiare o da portare via. Inoltre, ogni giorno a pranzo viene proposto un piatto del giorno. Un po' di Gran Bretagna nella Drôme."
    ],
    "fr": [
      "Salon de thé anglais avec de la pâtisserie à manger sur place ou à emporter. Plus un plat du jour  tous les midis. Un petit peu de Grande Bretagne dans la Drôme."
    ],
    "es": [
      "Salón de té inglés con bollería para comer o llevar. Además de un plato del día cada mediodía. Un poco de Gran Bretaña en la Drôme."
    ],
    "nl": [
      "Engelse tearoom met gebak om binnen te eten of mee te nemen. Plus elke lunch een dagschotel. Een beetje Groot-Brittannië in de Drôme."
    ]
  },
  "rdfs:label": {
