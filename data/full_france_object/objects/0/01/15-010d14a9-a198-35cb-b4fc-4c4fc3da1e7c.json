  "rdfs:comment": {
    "de": [
      "Erholen Sie sich in der Natur und genießen Sie die wohltuende Wirkung des Waldbadens."
    ],
    "en": [
      "Rejuvenate in the middle of nature and enjoy the beneficial effects of the forest bath."
    ],
    "it": [
      "Ringiovanite in mezzo alla natura e godetevi gli effetti benefici dei bagni nella foresta."
    ],
    "fr": [
      "Ressourcez-vous en pleine nature et appréciez les les effets bénéfiques du bain de forêt."
    ],
    "nl": [
      "Verjongen midden in de natuur en genieten van de heilzame werking van het bosbad."
    ],
    "es": [
      "Rejuvenezca en plena naturaleza y disfrute de los efectos beneficiosos de los baños de bosque."
    ]
  },
