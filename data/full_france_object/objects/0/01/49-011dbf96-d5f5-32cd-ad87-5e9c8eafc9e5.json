  "rdfs:comment": {
    "de": [
      "Traditionelles Restaurant mit regionalen Produkten vor den Toren von Ancenis. Restaurant von Montagmittag bis Freitagabend geöffnet. Empfang von Geschäftsessen, Gruppen, Einzelkunden. Herzlicher und authentischer Empfang, schmackhafte und saisonale Küche."
    ],
    "en": [
      "Traditional restaurant with local produce just outside Ancenis. Restaurant open Monday lunchtime to Friday evening. Catering for business meals, groups and individuals. Warm and authentic welcome, tasty seasonal cuisine."
    ],
    "it": [
      "Ristorante tradizionale con prodotti locali alle porte di Ancenis. Il ristorante è aperto dal lunedì a pranzo al venerdì sera. Si occupa di pranzi di lavoro, gruppi e singoli. Accoglienza calorosa e autentica, gustosa cucina di stagione."
    ],
    "fr": [
      "Restaurant traditionnel avec produits du terroir aux portes d'Ancenis. Restaurant ouvert du lundi midi au vendredi soir. Accueil de repas d'affaires, groupes, clientèle individuelle. Accueil chaleureux et authentique, cuisine savoureuse et de saison."
    ],
    "nl": [
      "Traditioneel restaurant met lokale producten net buiten Ancenis. Restaurant geopend van maandagmiddag tot vrijdagavond. Catering voor zakelijke maaltijden, groepen en individuen. Warm en authentiek onthaal, smakelijke seizoenskeuken."
    ],
    "es": [
      "Restaurante tradicional con productos locales a las afueras de Ancenis. Restaurante abierto de lunes a mediodía a viernes por la noche. Catering para comidas de negocios, grupos y particulares. Acogida cálida y auténtica, sabrosa cocina de temporada."
    ]
  },
