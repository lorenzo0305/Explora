  "rdfs:comment": {
    "de": [
      "Notre Dame des Hauteurs in der Gemeinde Golinhac bietet Ihnen einen wunderbaren Ausblick auf das Tal des Lot und die Berge von Aubrac."
    ],
    "en": [
      "Notre Dame des Hauteurs, in the commune of Golinhac, offers marvellous panoramic views over the Lot valley and the Aubrac mountains."
    ],
    "it": [
      "Notre Dame des Hauteurs, nel comune di Golinhac, offre meravigliose viste panoramiche sulla valle del Lot e sulle montagne di Aubrac."
    ],
    "fr": [
      "Notre Dame des Hauteurs, sur la commune de Golinhac, vous offrira un merveilleux panorama sur la vallée du Lot et les monts d'Aubrac."
    ],
    "nl": [
      "Notre Dame des Hauteurs, in de gemeente Golinhac, biedt een prachtig panoramisch uitzicht over de Lot-vallei en de bergen van Aubrac."
    ],
    "es": [
      "Notre Dame des Hauteurs, en el municipio de Golinhac, ofrece maravillosas vistas panorámicas sobre el valle del Lot y las montañas de Aubrac."
    ]
  },
