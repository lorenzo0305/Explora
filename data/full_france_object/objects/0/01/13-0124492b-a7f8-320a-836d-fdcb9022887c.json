  "rdfs:comment": {
    "de": [
      "Restaurant mit einer geselligen, gemeinsamen, bodenständigen Küche mit gereiftem Fleisch und einer schönen Weinkarte. Die Gerichte werden auf der Glut gegart."
    ],
    "en": [
      "Restaurant offering friendly, sharing, terroir cuisine, mature meats and a fine wine list. All dishes are braised."
    ],
    "it": [
      "Ristorante che offre una cucina locale conviviale e condivisa, carni stagionate e un'ottima carta dei vini. Tutti i piatti sono cucinati a fuoco aperto."
    ],
    "fr": [
      "Restaurant proposant une cuisine conviviale, de partage, de terroir, de viandes maturées, avec une belle carte de vins. Les cuissons sont faites à la braise."
    ],
    "es": [
      "Restaurante que ofrece una cocina local agradable y para compartir, carnes maduradas y una excelente carta de vinos. Todos los platos se cocinan al fuego."
    ],
    "nl": [
      "Restaurant met een vriendelijke, gedeelde, lokale keuken, gerijpt vlees en een goede wijnkaart. Alle gerechten worden boven open vuur bereid."
    ]
  },
