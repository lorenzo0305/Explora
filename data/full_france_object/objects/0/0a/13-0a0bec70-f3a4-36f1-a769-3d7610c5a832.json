  "rdfs:comment": {
    "de": [
      "Gemeinschaftsbecken dienten den Bewohnern der Weiler als fließende Wasserstellen oder sogar zum Wäschewaschen."
    ],
    "en": [
      "Basins were used by the villagers as a water supply point and, also, to wash their linen and clothes."
    ],
    "it": [
      "Le piscine comunitarie venivano utilizzate dagli abitanti dei villaggi per fornire acqua corrente e persino per lavare i vestiti."
    ],
    "fr": [
      "Bassins communautaires servaient aux habitants des hameaux comme points d'eau courante, voire pour y laver leur linge."
    ],
    "es": [
      "Los habitantes de las aldeas utilizaban las piscinas comunitarias para abastecerse de agua corriente e incluso para lavar la ropa."
    ],
    "nl": [
      "De inwoners van de gehuchten gebruikten de gemeenschappelijke vijvers als stromend water en zelfs om hun kleren te wassen."
    ]
  },
