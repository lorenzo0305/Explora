  "rdfs:comment": {
    "de": [
      "Das Restaurant Pitch & Putt befindet sich im Herzen des Golfplatzes von Saint-Ouen-l'Aumône und bietet eine grüne und friedliche Umgebung, die ideal für eine Gourmetpause ist."
    ],
    "en": [
      "The Pitch & Putt restaurant is located in the heart of the Saint-Ouen-l'Aumône golf course, offering a peaceful green setting ideal for a gourmet break."
    ],
    "it": [
      "Il ristorante Pitch & Putt si trova nel cuore del campo da golf di Saint-Ouen-l'Aumône e offre un ambiente tranquillo e verdeggiante, ideale per una pausa gourmet."
    ],
    "fr": [
      "Le restaurant Pitch & Putt est situé au cœur du golf de Saint-Ouen-l'Aumône, offrant un cadre verdoyant et paisible, idéal pour une pause gourmande."
    ],
    "nl": [
      "Het Pitch & Putt restaurant ligt in het hart van de golfbaan van Saint-Ouen-l'Aumône en biedt een rustige, groene omgeving die ideaal is voor een gastronomische pauze."
    ],
    "es": [
      "El restaurante Pitch & Putt, situado en el corazón del campo de golf de Saint-Ouen-l'Aumône, ofrece un entorno tranquilo y frondoso ideal para una escapada gastronómica."
    ]
  },
