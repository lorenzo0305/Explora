  "rdfs:comment": {
    "de": [
      "Gastronomisches Restaurant, Maître Restaurateur in Nort sur Erdre zwischen Loire und Kanal von Nantes nach Brest, hausgemachte Küche, und bei schönem Wetter Terrasse unter hundertjährigen Platanen."
    ],
    "en": [
      "Gastronomic restaurant, Maître Restaurateur in Nort sur Erdre, between the Loire and the Nantes-Brest Canal, home cooking and, in fine weather, a terrace under century-old plane trees."
    ],
    "it": [
      "Ristorante gastronomico, Maître Restaurateur a Nort sur Erdre tra la Loira e il Canal de Nantes à Brest, cucina casalinga e, nella bella stagione, terrazza sotto platani secolari."
    ],
    "fr": [
      "Restaurant Gastronomique, Maître Restaurateur à Nort sur Erdre entre Loire et Canal de Nantes à Brest, cuisine maison, et au beau jour terrasse sous des platanes centenaires."
    ],
    "es": [
      "Restaurante gastronómico, Maître Restaurateur en Nort sur Erdre entre el Loira y el Canal de Nantes à Brest, cocina casera y, cuando hace buen tiempo, terraza bajo plátanos centenarios."
    ],
    "nl": [
      "Gastronomisch restaurant, Maître Restaurateur in Nort sur Erdre tussen de Loire en het Canal de Nantes à Brest, huiselijke keuken en bij mooi weer een terras onder eeuwenoude platanen."
    ]
