  "rdfs:comment": {
    "de": [
      "Restaurant, das eine abwechslungsreiche Speisekarte mit traditionellen Gerichten anbietet. Lassen Sie sich seine Spezialität nicht entgehen: die Dampfnudeln."
    ],
    "en": [
      "Restaurant offering a varied menu of traditional dishes. Don't miss its specialty: Dampfnüdles."
    ],
    "it": [
      "Ristorante che offre un menù vario di piatti tradizionali. Da non perdere la sua specialità: il Dampfnüdles."
    ],
    "fr": [
      "Restaurant proposant une carte variée avec des plats traditionnels. Ne passez pas à côté de sa spécialité : les Dampfnüdles."
    ],
    "es": [
      "Restaurante que ofrece una variada carta de platos tradicionales. No te pierdas su especialidad: Dampfnüdles."
    ],
    "nl": [
      "Restaurant met een gevarieerd menu van traditionele gerechten. Mis de specialiteit niet: Dampfnüdles."
    ]
