      "dc:description": {
        "de": [
          "35 Lebensmittelstände werden angeboten: Obst und Gemüse, Braten, Feinkost, Fertiggerichte, Lebensmittel, Oliven, Konserven, Käse-Creme, Konditorei, Bäckerei."
        ],
        "en": [
          "35 food stalls: fruit and vegetables, rotisserie, delicatessen, ready-cooked meals, groceries, olives, preserves, cheese, pastries and bakery."
        ],
        "it": [
          "35 stand gastronomici: frutta e verdura, rosticceria, gastronomia, piatti pronti, alimentari, olive, conserve, formaggi, pasticceria e panetteria."
        ],
        "fr": [
          "35 étals alimentaires sont proposés : fruits et légumes, rôtisserie, traiteur, plats cuisinés, épicerie, olives, conserves, crèmerie fromages, pâtisserie, boulangerie."
        ],
        "es": [
          "35 puestos de alimentación: frutas y verduras, asadores, charcutería, platos preparados, ultramarinos, aceitunas, conservas, quesos, bollería y panadería."
        ],
        "nl": [
          "35 eetkraampjes: fruit en groenten, rotisserie, delicatessen, kant-en-klaarmaaltijden, kruidenierswaren, olijven, conserven, kaas, gebak en bakkerij."
        ]
      },
