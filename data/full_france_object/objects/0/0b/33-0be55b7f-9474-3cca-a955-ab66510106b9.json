  "rdfs:comment": {
    "de": [
      "Cycles Isard ist eine Fahrradreparaturwerkstatt, die sich auf die Herstellung von Fahrradrädern nach Maß spezialisiert hat."
    ],
    "en": [
      "Cycles Isard is a bicycle repair shop specializing in the manufacture of custom bicycle wheels."
    ],
    "it": [
      "Cycles Isard è un negozio di riparazione di biciclette specializzato nella produzione di ruote per biciclette su misura."
    ],
    "fr": [
      "Cycles Isard est un atelier de réparation vélo spécialisé dans la fabrication de roues de vélos sur mesure."
    ],
    "nl": [
      "Cycles Isard is een fietsenmaker gespecialiseerd in het maken van fietswielen op maat."
    ],
    "es": [
      "Cycles Isard es un taller de reparación de bicicletas especializado en la fabricación de ruedas de bicicleta a medida."
    ]
  },
