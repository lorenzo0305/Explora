  "rdfs:comment": {
    "de": [
      "Gehen Sie mit einem unserer Experten einen halben Tag lang auf Entdeckungsreise zum Pyrenäenbären! Lernen Sie die emblematische Tierart der Pyrenäen kennen."
    ],
    "en": [
      "Spend half a day discovering the Pyrenean bear with one of our experts! Come and discover the emblematic species of the Pyrenees."
    ],
    "it": [
      "Trascorrete una mezza giornata alla scoperta dell'orso pirenaico con uno dei nostri esperti! Venite a scoprire la specie emblematica dei Pirenei."
    ],
    "fr": [
      "Partez pendant une demi-journée à la découverte de l'ours des Pyrénées, avec un de nos experts ! Venez découvrir l'espèce emblématique des Pyrénées."
    ],
    "es": [
      "¡Pase media jornada descubriendo el oso pirenaico con uno de nuestros expertos! Ven a descubrir la especie emblemática de los Pirineos."
    ],
    "nl": [
      "Breng een halve dag door met het ontdekken van de Pyreneese beer met een van onze experts! Kom en ontdek de emblematische diersoort van de Pyreneeën."
    ]
  },
