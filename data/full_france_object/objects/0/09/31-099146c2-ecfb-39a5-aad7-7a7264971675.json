  "rdfs:comment": {
    "de": [
      "Als bäuerliche Bäcker verarbeiten wir unsere Ernte aus alten Weizensorten auf dem Bauernhof zu BROT. Unser Brot besteht zu 100 % aus Natursauerteig."
    ],
    "en": [
      "As farmer-bakers, we transform our harvest of old wheat into bread on the farm. Our bread is 100% natural leaven."
    ],
    "it": [
      "Come agricoltori-fornai, trasformiamo il nostro raccolto di grano antico in pane in fattoria. Il nostro pane è al 100% a lievitazione naturale."
    ],
    "fr": [
      "Paysans boulangers, nous transformons notre récolte de blés anciens en PAIN à la ferme. Notre pain est 100% levain naturel."
    ],
    "es": [
      "Como agricultores-panaderos, transformamos nuestra cosecha de trigo viejo en pan en la granja. Nuestro pan es 100% de levadura natural."
    ],
    "nl": [
      "Als boerenbakkers zetten wij onze oogst van oude tarwe om in brood op de boerderij. Ons brood is 100% natuurlijk zuurdesem."
    ]
  },
