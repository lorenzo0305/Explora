  "rdfs:comment": {
    "de": [
      "Restaurant/Bar im Herzen des Dorfes Avrieux (4 km von La Norma entfernt), mit Kamin, schöner Terrasse und kostenlosem Parkplatz in der Nähe. Traditionelle französische Küche. Reservierungen werden empfohlen."
    ],
    "en": [
      "Restaurant/bar in the heart of the village of Avrieux (4 km from La Norma), with fireplace, lovely terrace and free parking nearby. Traditional French cuisine. Reservations recommended."
    ],
    "it": [
      "Ristorante/bar nel cuore del villaggio di Avrieux (4 km da La Norma), con camino aperto, bella terrazza e parcheggio gratuito nelle vicinanze. Cucina tradizionale francese. Si consiglia la prenotazione."
    ],
    "fr": [
      "Restaurant/bar au cœur du village d'Avrieux (à 4 km de La Norma), avec cheminée, belle terrasse et parking gratuit à proximité. Cuisine française traditionnelle. Réservations recommandées."
    ],
    "es": [
      "Restaurante/bar en el corazón del pueblo de Avrieux (a 4 km de La Norma), con chimenea, bonita terraza y aparcamiento gratuito en las inmediaciones. Cocina tradicional francesa. Se recomienda reservar."
    ],
    "nl": [
      "Restaurant/bar in het hart van het dorp Avrieux (4 km van La Norma), met open haard, mooi terras en gratis parkeergelegenheid in de buurt. Traditionele Franse keuken. Reserveren aanbevolen."
    ]
  },
