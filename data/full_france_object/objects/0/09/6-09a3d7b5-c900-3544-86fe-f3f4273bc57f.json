      "dc:description": {
        "de": [
          "Charaktervoller Wohnsitz in Morvandelle auf dem Jakobsweg. Herzlicher Empfang in aller Einfachheit, erholsame und grüne Umgebung."
        ],
        "en": [
          "Morvandelle residence of character located on the road to Santiago de Compostela. Warm welcome in all simplicity, restful and green surroundings"
        ],
        "it": [
          "Casa Morvan di carattere situata sul cammino di Santiago de Compostela. Accoglienza calorosa in tutta semplicità, ambiente riposante e verde"
        ],
        "fr": [
          "Demeure morvandelle de caractère située sur le chemin de St Jacques de Compostelle. Accueil chaleureux en toute simplicité, cadre reposant et verdoyant. "
        ],
        "nl": [
          "Karaktervol huis in Morvan op de weg naar Santiago de Compostela. Een warm welkom in alle eenvoud, een rustgevende en groene omgeving"
        ],
        "es": [
          "Casa de carácter moruno situada en el camino de Santiago de Compostela. Acogida cálida con toda sencillez, entorno tranquilo y verde"
        ]
      },
