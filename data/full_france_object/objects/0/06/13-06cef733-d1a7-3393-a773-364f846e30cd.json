  "rdfs:comment": {
    "de": [
      "Inmitten eines Pinienwaldes, direkter Zugang zum Strand, 5 Minuten mit dem Auto, 10 Minuten mit dem Fahrrad über den Radweg, vom Dorf und vom Jachthafen entfernt."
    ],
    "en": [
      "In the heart of a pine forest, direct access to the beach, 5 minutes by car, 10 minutes by bike from the village and marina."
    ],
    "it": [
      "Nel cuore di una pineta, con accesso diretto alla spiaggia, a 5 minuti in auto e 10 minuti in bicicletta dal villaggio e dal porto turistico."
    ],
    "fr": [
      "Au cœur d'une forêt de pins, accès direct à la plage, à 5 minutes en voiture, 10 min en vélo par la piste cyclable, du village et du port de plaisance."
    ],
    "nl": [
      "In het hart van een dennenbos, met directe toegang tot het strand, op 5 minuten met de auto en 10 minuten met de fiets van het dorp en de jachthaven."
    ],
    "es": [
      "En el corazón de un pinar, con acceso directo a la playa, a 5 minutos en coche y 10 minutos en bicicleta del pueblo y del puerto deportivo."
    ]
  },
