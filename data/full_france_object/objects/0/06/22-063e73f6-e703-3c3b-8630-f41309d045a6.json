  "rdfs:comment": {
    "de": [
      "Komödie von Mohamed Hamidi. Der algerische Kleinbauer Fatah erhält eine Einladung, seine Kuh Jacqueline zur Landwirtschaftsmesse in Paris zu bringen..\nFür alle Zuschauer"
    ],
    "en": [
      "Comedy by Mohamed Hamidi. Fatah, a small Algerian farmer, receives an invitation to take his cow Jacqueline to the Salon de l?Agriculture in Paris?\nAll audiences"
    ],
    "it": [
      "Commedia di Mohamed Hamidi. Fatah, un piccolo agricoltore algerino, riceve l'invito a portare la sua mucca Jacqueline al Salone dell'Agricoltura di Parigi?\nAdatto a tutti i tipi di pubblico"
    ],
    "fr": [
      "Comédie de Mohamed Hamidi. Fatah, petit paysan algérien, reçoit une invitation pour amener sa vache Jacqueline au Salon de l’Agriculture à Paris… \nTout public"
    ],
    "nl": [
      "Komedie van Mohamed Hamidi. Fatah, een kleine Algerijnse boer, krijgt een uitnodiging om met zijn koe Jacqueline naar de Parijse landbouwshow te gaan?\nGeschikt voor alle doelgroepen"
    ],
    "es": [
      "Comedia de Mohamed Hamidi. Fatah, un pequeño granjero argelino, recibe una invitación para llevar a su vaca Jacqueline al Salón de la Agricultura de París?\nApta para todos los públicos"
    ]
  },
