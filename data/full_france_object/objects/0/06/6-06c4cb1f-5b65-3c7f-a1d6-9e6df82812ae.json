      "dc:description": {
        "de": [
          "Die Latino-Sonntage mit Chris Paggi in der Domaine de la cabane verte am Lac des Settons. Von 18.30 bis 19.30 Uhr Einführung Salsa/Bachata/Merengue. Von 19:30 bis 2:00 Uhr Latino-Abend. Bar - Restauration und Tapas-Essen auf Reservierung."
        ],
        "en": [
          "Latino Sundays with Chris Paggi at the Domaine de la cabane verte at Lac des Settons. 6.30pm to 7.30pm Salsa/Bachata/Merengue initiation. From 7.30pm to 2am Latino evening. Bar - catering and tapas meal on reservation."
        ],
        "it": [
          "Domenica latina con Chris Paggi al Domaine de la cabane verte al Lac des Settons. Dalle 18.30 alle 19.30 iniziazione alla Salsa/Bachata/Merengue. Dalle 19.30 alle 2.00 serata latina. Bar - catering e tapas su prenotazione."
        ],
        "fr": [
          "Les dimanches latino avec Chris Paggi au Domaine de la cabane verte au lac des Settons. de 18h30 à 19h30 Initiation Salsa/Bachata/Merengue. De 19h30 à 2hh Soirée Latino. Bar - restauration et repas tapas sur réservation."
        ],
        "nl": [
          "Latino zondagen met Chris Paggi op het Domaine de la cabane verte bij Lac des Settons. 18.30 tot 19.30 uur initiatie Salsa/Bachata/Merengue. Van 19.30 tot 02.00 uur Latino-avond. Bar - catering en tapasmaaltijd op reservering."
        ],
        "es": [
          "Domingos latinos con Chris Paggi en el Domaine de la cabane verte en Lac des Settons. De 18.30 a 19.30 h Iniciación a la salsa/bahata/merengue. De 19h30 a 2h de la madrugada noche latina. Bar - catering y comida de tapas previa reserva."
        ]
      },
