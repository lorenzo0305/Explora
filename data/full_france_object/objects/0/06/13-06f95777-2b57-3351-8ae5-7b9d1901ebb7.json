  "rdfs:comment": {
    "de": [
      "Im Herzen von Aubenas, 200 Meter vom Schloss entfernt, empfängt Sie das Restaurant La Bouch'en Coin und bietet Ihnen eine hausgemachte Küche aus frischen Produkten."
    ],
    "en": [
      "Traditional French cuisine prepared on the premises with fresh local produce. \r\nRestaurant situated in the town centre of Aubenas, just 200m from the castle.\r\nPets welcome. \r\nEnglish spoken."
    ],
    "it": [
      "Situato nel cuore di Aubenas, a 200 metri dal castello, il ristorante La Bouch'en Coin vi accoglie e offre una cucina casalinga a base di prodotti freschi."
    ],
    "fr": [
      "Situé au cœur d'Aubenas, à 200 mètres du château, le restaurant La Bouch'en Coin vous accueille et vous propose une cuisine maison à base de produits frais."
    ],
    "es": [
      "Situado en el corazón de Aubenas, a 200 metros del castillo, el restaurante La Bouch'en Coin le da la bienvenida y le ofrece una cocina casera con productos frescos."
    ],
    "nl": [
      "Het restaurant La Bouch'en Coin, gelegen in het hart van Aubenas, op 200 meter van het kasteel, verwelkomt u en biedt een huisgemaakte keuken op basis van verse producten."
    ]
  },
