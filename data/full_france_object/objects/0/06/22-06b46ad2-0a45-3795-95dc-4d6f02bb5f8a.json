  "rdfs:comment": {
    "de": [
      "Ein Weihnachtskonzert mit den Solisten Darron und Katy Batho (Royal Opera House), begleitet von der Voix du Ch?ur de Lamolayrette und einem besonderen Gast, Guillaume Roussilhe. Inklusive Glühwein für eine warme und festliche Atmosphäre."
    ],
    "en": [
      "A Christmas concert with soloists Darron and Katy Batho (Royal Opera House), accompanied by the Voix du Ch?ur de Lamolayrette and special guest Guillaume Roussilhe. Mulled wine included for a warm, festive atmosphere."
    ],
    "it": [
      "Concerto di Natale con i solisti Darron e Katy Batho (Royal Opera House), accompagnati dalla Voix du Ch?ur de Lamolayrette e dall'ospite speciale Guillaume Roussilhe. Vin brulé incluso per un'atmosfera calda e festosa."
    ],
    "fr": [
      "Un concert de Noël avec les solistes Darron et Katy Batho (Royal Opera House), accompagné par la Voix du Chœur de Lamolayrette et un invité spécial, Guillaume Roussilhe. Vin chaud inclus pour une ambiance chaleureuse et festive."
    ],
    "nl": [
      "Een kerstconcert met solisten Darron en Katy Batho (Royal Opera House), begeleid door de Voix du Ch?ur de Lamolayrette en speciale gast Guillaume Roussilhe. Glühwein is inbegrepen voor een warme en feestelijke sfeer."
    ],
    "es": [
      "Concierto de Navidad con los solistas Darron y Katy Batho (Royal Opera House), acompañados por la Voix du Chœur de Lamolayrette y el invitado especial Guillaume Roussilhe. Vino caliente incluido para un ambiente cálido y festivo."
    ]
  },
