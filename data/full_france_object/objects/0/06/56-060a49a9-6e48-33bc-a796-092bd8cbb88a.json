  "rdfs:comment": {
    "de": [
      "Ein festlicher Pop-Rock-Abend? Tauchen Sie ein in die Hits der 80er Jahre mit Goldman, Queen oder Balavoine, die von Profis interpretiert werden. Getränke und Stimmung sind garantiert! Kostenlos."
    ],
    "en": [
      "A festive Pop Rock evening! Relive the hits of the 80s with Goldman, Queen or Balavoine, performed by the pros. Refreshments and atmosphere guaranteed! Free admission."
    ],
    "it": [
      "Una serata di festa all'insegna del Pop Rock! Rivivete i successi degli anni '80 con Goldman, Queen e Balavoine, interpretati da professionisti. Rinfresco e atmosfera garantiti! Ingresso libero."
    ],
    "fr": [
      "Soirée Pop Rock festive ! Replongez dans les hits des années 80 avec Goldman, Queen ou Balavoine, interprétés par des pros. Buvette et ambiance garantie ! Gratuit."
    ],
    "es": [
      "¡Una velada festiva de Pop Rock! Reviva los éxitos de los 80 con Goldman, Queen y Balavoine, interpretados por profesionales. Refrescos y ambiente garantizados Entrada gratuita."
    ],
    "nl": [
      "Een feestelijke Pop Rock avond! Herbeleef de hits van de jaren 80 met Goldman, Queen en Balavoine, uitgevoerd door de pro's. Versnaperingen en sfeer gegarandeerd! Gratis toegang."
    ]
  },
