  "rdfs:comment": {
    "de": [
      "Gaëlle und Steve betreiben eine handwerkliche Konservenfabrik für saisonales und biologisches Obst und Gemüse. Sie freuen sich, Ihnen ihre verschiedenen Rezepte in der Touristeninformation vorzustellen."
    ],
    "en": [
      "Gaëlle and Steve run an artisanal cannery selling seasonal and organic fruit and vegetables. They are happy to share their recipes with you at the tourist office."
    ],
    "it": [
      "Gaëlle e Steve gestiscono una tradizionale fabbrica di conserve che vende frutta e verdura di stagione e biologica. Saranno lieti di mostrarvi le loro varie ricette presso l'ufficio turistico."
    ],
    "fr": [
      "Gaëlle et Steve ont une conserverie artisanale de fruits et légumes de saison et bio. Ils sont heureux de vous faire découvrir leurs différentes recettes au sein de l'office de tourisme."
    ],
    "es": [
      "Gaëlle y Steve dirigen una conservera tradicional que vende frutas y verduras de temporada y ecológicas. Estarán encantados de enseñarte sus diversas recetas en la oficina de turismo."
    ],
    "nl": [
      "Gaëlle en Steve runnen een traditionele conservenfabriek waar ze seizoensgebonden en biologische groenten en fruit verkopen. Bij het toeristenbureau laten ze je graag hun verschillende recepten zien."
    ]
  },
