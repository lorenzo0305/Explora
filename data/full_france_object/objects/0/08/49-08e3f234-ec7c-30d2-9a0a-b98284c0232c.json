  "rdfs:comment": {
    "de": [
      "3-Sterne-Ferienhaus in idealer Lage, 10 Minuten vom Puy du Fou-Park, 1 Stunde von den Stränden und dem Marais Poitevin entfernt. Wanderwege in der Umgebung, zahlreiche Besichtigungen."
    ],
    "en": [
      "3-star gîte ideally located, 10 minutes from the Puy du Fou park, 1 hour from the beaches and Marais Poitevin. Nearby footpaths, numerous visits."
    ],
    "it": [
      "Gîte 3 stelle situato in posizione ideale, a 10 minuti dal parco Puy du Fou, a 1 ora dalle spiagge e dal Marais Poitevin. Sentieri nelle vicinanze, numerose visite."
    ],
    "fr": [
      "Gîte 3 étoiles idéalement bien situé, 10 minutes du parc du Puy du Fou, 1h des plages et du Marais poitevin. Sentiers pédestres aux alentours, nombreuses visites."
    ],
    "es": [
      "Casa rural de 3 estrellas idealmente situada, a 10 minutos del parque del Puy du Fou, a 1 hora de las playas y del Marais Poitevin. Senderos en los alrededores, numerosas visitas."
    ],
    "nl": [
      "3-sterren gîte ideaal gelegen, op 10 minuten van het Puy du Fou park, 1 uur van de stranden en het Marais Poitevin. Wandelpaden in de omgeving, talrijke bezoeken."
    ]
  },
