  "rdfs:comment": {
    "de": [
      "Ein familienfreundlicher, von Mauern geschützter Campingplatz, der umgeben von Weinfeldern 500 m vom Strand entfernt liegt. Überdachter und beheizter Swimmingpool."
    ],
    "en": [
      "A family campsite which is entirely walled-in and surrounded with vineyards. It is situated 500m from the beach and features a heated covered swimming-pool."
    ],
    "it": [
      "Camping familiare interamente recintato e circondato da campi di vigneti, a 500 m dalla spiaggia, con piscina coperta e riscaldata."
    ],
    "fr": [
      "Camping familial entièrement clos de mur et entouré de champs de vignes, à 500m de la plage de sable fin, avec piscine couverte et chauffée. Accès direct sur les pistes cyclables au milieu de l'ile de Ré."
    ],
    "nl": [
      "Familiecamping geheel ommuurd en omgeven door wijngaarden, op 500m van het zandstrand, met een overdekt en verwarmd zwembad. Directe toegang tot de fietspaden in het midden van het Ile de Ré."
    ],
    "es": [
      "Camping familiar completamente cerrado y rodeado por campos de viñas, a 500m de la playa, con piscina cubierta y calentada."
    ]
  },
