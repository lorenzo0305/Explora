      "dc:description": {
        "de": [
          "Freitag, 15. August: LAGESSE – Jährliches Spanferkelessen vom Spieß. Organisiert vom Lagesse-Festivalkomitee. Kontakt per Telefon: +33 (0)7 69 62 56 41 oder per E-Mail: contact@othe-armance.fr"
        ],
        "en": [
          "Friday, August 15: LAGESSE - Annual meal of spit-roast suckling pig. Organized by the Lagesse Festival Committee. Contact by phone: +33 (0)7 69 62 56 41 or by email: contact@othe-armance.fr"
        ],
        "it": [
          "Venerdì 15 agosto: LAGESSE - Pasto annuale di maialino da latte allo spiedo. Organizzato dal Comitato del Festival Lagesse. Contatto telefonico: +33 (0)7 69 62 56 41 o via email: contact@othe-armance.fr"
        ],
        "fr": [
          "Vendredi 15 août : LAGESSE - Repas annuel Cochon de lait à la broche. Organisé par le Comité des fêtes de Lagesse. Contact par téléphone : +33 (0)7 69 62 56 41 ou par mail : contact@othe-armance.fr"
        ],
        "nl": [
          "Vrijdag 15 augustus: LAGESSE - Jaarlijkse maaltijd van aan het spit gebraden speenvarken. Georganiseerd door het Lagesse Festivalcomité. Contact per telefoon: +33 (0)7 69 62 56 41 of per e-mail: contact@othe-armance.fr"
        ],
        "es": [
          "Viernes 15 de agosto: LAGESSE - Comida anual del cochinillo asado. Organiza el Comité des fêtes de Lagesse. Contacto por teléfono: +33 (0)7 69 62 56 41 o por correo electrónico: contact@othe-armance.fr"
        ]
      },
