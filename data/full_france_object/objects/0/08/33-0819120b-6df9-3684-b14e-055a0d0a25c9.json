  "rdfs:comment": {
    "de": [
      "Der Reitstall En Sans befindet sich in Cadours auf einem 8 Hektar großen Gelände und bietet Ihnen Ausritte, Pferdepensionen und Reitunterricht an."
    ],
    "en": [
      "The En Sans stables located in Cadours on an 8-hectare estate offer horse rides, horse boarding and riding lessons."
    ],
    "it": [
      "Il maneggio En Sans, situato a Cadours in una tenuta di 8 ettari, offre equitazione, pensione e lezioni di equitazione."
    ],
    "fr": [
      "Les écuries d'En Sans situées à Cadours sur un domaine de 8 hectares vous proposent des balades à cheval,  pensions pour chevaux, et leçons d´équitation."
    ],
    "es": [
      "Los establos de En Sans, situados en Cadours, en una finca de 8 hectáreas, ofrecen paseos a caballo, pupilaje y clases de equitación."
    ],
    "nl": [
      "De manege En Sans, gelegen in Cadours op een landgoed van 8 hectare, biedt paardrijden, paardenpension en paardrijlessen."
    ]
  },
