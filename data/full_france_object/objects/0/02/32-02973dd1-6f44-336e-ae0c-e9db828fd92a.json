  "rdfs:comment": {
    "de": [
      "Erkunde die Welt der Manga mit dem Mangaka Auleeve, der dir beibringt, wie du atypische Charaktere mithilfe der grafischen Codes des Manga zeichnest."
    ],
    "en": [
      "Explore the world of manga with mangaka Auleeve, who will teach you how to draw atypical characters using the graphic codes of manga."
    ],
    "it": [
      "Esplorate il mondo dei manga con il mangaka Auleeve, che vi insegnerà a disegnare personaggi insoliti utilizzando i codici grafici dei manga."
    ],
    "fr": [
      "Explorez l'univers du manga avec le mangaka Auleeve, qui vous apprendra à dessiner des personnages atypiques en utilisant les codes graphiques du manga."
    ],
    "nl": [
      "Verken de wereld van manga met mangaka Auleeve, die je zal leren hoe je ongewone personages kunt tekenen met behulp van de grafische codes van manga."
    ],
    "es": [
      "Explore el mundo del manga con el mangaka Auleeve, que le enseñará a dibujar personajes insólitos utilizando los códigos gráficos del manga."
