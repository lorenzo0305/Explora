  "rdfs:comment": {
    "de": [
      "Studio mit Kabine für 2/3 Personen, im Erdgeschoss mit Ostausrichtung und kleinem Garten von 3,70 m², in der Nähe der Geschäfte, der Thermen und am Fuße der Seilbahn, die nach Superbagneres führt."
    ],
    "en": [
      "Studio cabin for 2/3 people, EAST-facing first floor with small 3.70 m² garden, close to shops, thermal baths and at the foot of the gondola to Superbagneres."
    ],
    "it": [
      "Cabina monolocale per 2/3 persone, al piano terra esposto a est con un piccolo giardino di 3,70 m², vicino ai negozi e alle terme e ai piedi della funivia per Superbagneres."
    ],
    "fr": [
      "Studio cabine pour 2/3 personnes, en rez-de-chaussée exposé EST avec petit jardinet de 3,70 m², proche des commerces, des thermes et au pied du télécabine menant à Superbagneres."
    ],
    "nl": [
      "Studio voor 2/3 personen, op de begane grond op het oosten met een kleine tuin van 3.70 m², dicht bij de winkels en de thermale baden en aan de voet van de kabelbaan naar Superbagneres."
    ],
    "es": [
      "Estudio cabina para 2/3 personas, en una planta baja orientada al este con un pequeño jardín de 3,70 m², cerca de los comercios y de los baños termales y al pie del teleférico de Superbagneres."
    ]
  },
