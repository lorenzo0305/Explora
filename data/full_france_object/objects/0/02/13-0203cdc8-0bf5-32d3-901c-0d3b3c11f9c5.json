  "rdfs:comment": {
    "de": [
      "Skulptur in situ von 3 Künstlern entworfen und realisiert :\r\n? Mireille Fulpius\r\n? Marc de Roover\r\n? Christophe Gonnet\r\n\r\nDer Eingang erfolgt über den Rundweg in der Nähe des Bois Lambert, ein kurzer Spaziergang von ca. 800 m Länge."
    ],
    "en": [
      "In situ sculpture designed and created by 3 artists:\r\n? Mireille Fulpius\r\n? Marc de Roover\r\n? Christophe Gonnet\r\n\r\nThe entrance is via the chemin de ronde, near Bois Lambert, a short stroll of around 800 m."
    ],
    "it": [
      "Scultura in situ progettata e realizzata da 3 artisti:\r\n? Mireille Fulpius\r\n? Marc de Roover\r\n? Christophe Gonnet\r\n\r\nL'ingresso è dal Chemin de Ronde, vicino al Bois Lambert, con una breve passeggiata di circa 800 m."
    ],
    "fr": [
      "Sculpture in situ conçue et réalisée par 3 artistes :\r\nΟ Mireille Fulpius\r\nΟ Marc de Roover\r\nΟ Christophe Gonnet\r\n\r\nL'entrée se fait par le chemin de ronde, près du Bois Lambert, une petite balade d'environ 800 m."
    ],
    "nl": [
      "In situ sculptuur ontworpen en gemaakt door 3 kunstenaars:\r\n? Mireille Fulpius\r\n? Marc de Roover\r\n? Christophe Gonnet\r\n\r\nDe ingang is via de Chemin de Ronde, vlakbij Bois Lambert, een korte wandeling van ongeveer 800 m."
    ],
    "es": [
      "Escultura in situ diseñada y creada por 3 artistas:\r\n? Mireille Fulpius\r\n? Marc de Roover\r\n? Christophe Gonnet\r\n\r\nLa entrada es por el Chemin de Ronde, cerca de Bois Lambert, un corto paseo de unos 800 m."
    ]
  },
