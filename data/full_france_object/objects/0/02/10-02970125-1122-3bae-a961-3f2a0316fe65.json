  "rdfs:comment": {
    "de": [
      "Ländliches Essen mit Tanz, organisiert von der Harmonie Bussang / Saint-Maurice. Musikalische Unterhaltung durch Martial VILLEMIN und seinen Saxophonisten. Reservierung per Telefon dringend empfohlen."
    ],
    "en": [
      "Country-style meal and dance organized by the Harmonie Bussang / Saint-Maurice. Musical entertainment by Martial VILLEMIN and his saxophonist. Reservations strongly recommended by telephone."
    ],
    "it": [
      "Pranzo e ballo in stile country organizzato dall'Harmonie Bussang / Saint-Maurice. Intrattenimento musicale a cura di Martial VILLEMIN e del suo sassofonista. Si consiglia vivamente di prenotare telefonicamente."
    ],
    "fr": [
      "Repas campagnard dansant organisé par l'Harmonie Bussang / Saint-Maurice. Animation musicale par Martial VILLEMIN et son saxophoniste. Réservation fortement conseillée par téléphone."
    ],
    "nl": [
      "Landelijke maaltijd en dans georganiseerd door de Harmonie Bussang / Saint-Maurice. Muzikale omlijsting door Martial VILLEMIN en zijn saxofonist. Telefonisch reserveren wordt sterk aanbevolen."
    ],
    "es": [
      "Comida campestre y baile organizado por la Harmonie Bussang / Saint-Maurice. Animación musical a cargo de Martial VILLEMIN y su saxofonista. Se recomienda encarecidamente reservar por teléfono."
    ]
  },
