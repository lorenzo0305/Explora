  "rdfs:comment": {
    "de": [
      "Tauchen Sie ein in die authentischen Aromen des Les Remparts de St-Rambert, wo unsere Küche frische, lokale und saisonale Produkte zur Geltung bringt."
    ],
    "en": [
      "Immerse yourself in authentic flavours at Remparts de St-Rambert, where our cuisine features fresh, local and seasonal produce."
    ],
    "it": [
      "Immergetevi nel cuore dei sapori autentici di Remparts de St-Rambert, dove la nostra cucina è caratterizzata da prodotti freschi, locali e di stagione."
    ],
    "fr": [
      "Plongez au cœur des saveurs authentiques aux Remparts de St-Rambert, où notre cuisine met en valeur des produits frais, locaux et de saison."
    ],
    "es": [
      "Sumérjase en el corazón de los sabores auténticos en Remparts de St-Rambert, donde nuestra cocina ofrece productos frescos, locales y de temporada."
    ],
    "nl": [
      "Duik in het hart van authentieke smaken bij Remparts de St-Rambert, waar onze keuken verse, lokale en seizoensgebonden producten bevat."
    ]
  },
