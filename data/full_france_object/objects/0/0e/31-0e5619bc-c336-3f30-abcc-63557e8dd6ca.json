  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/d05b438d-fa54-3dd6-9524-f1665b6e90a4",
      "dc:description": {
        "de": [
          "1. Stock: Küche, Wohnzimmer, (Sofa, Fernseher), 1 Schlafzimmer (1 Bett 140), Balkon, Badezimmer. WC (Waschmaschine).\nParkplatz in der Nähe."
        ],
        "en": [
          "First floor: kitchen, living room (sofa, TV), bedroom with double bed. Balcony, shower room, toilets (washing machine). Electric heating (not included). Car park nearby."
        ],
        "it": [
          "1° piano: cucina, soggiorno (divano, televisione), 1 camera (1 letto matrimoniale), balcone, bagno con doccia. WC (lavatrice).\nParcheggio nelle vicinanze."
        ],
        "fr": [
          "1er étage : cuisine, salle de séjour, (canapé, télévision), 1 chambre (1 lit 140), balcon, salle d'eau. WC (lave-linge).\nParking à proximité."
        ],
        "nl": [
          "1ste verdieping: keuken, woonkamer (bank, televisie), 1 slaapkamer (1 2-persoonsbed), balkon, doucheruimte. WC (wasmachine).\nParkeerplaats in de buurt."
        ],
        "es": [
          "1er piso: cocina, salón (sofá, televisión), 1 dormitorio (1 cama doble), balcón, cuarto de ducha. WC (lavadora).\nAparcamiento en las cercanías."
        ]
      },
