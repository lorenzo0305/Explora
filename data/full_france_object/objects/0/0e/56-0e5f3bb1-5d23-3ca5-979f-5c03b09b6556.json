  "rdfs:comment": {
    "de": [
      "Die Saxophonistin Géraldine Laurent beschäftigt sich mit der Musik von Charles Mingus, wie sie es vor zehn Jahren mit der Musik von Charlie Parker getan hat."
    ],
    "en": [
      "Saxophonist Géraldine Laurent turns her attention to the music of Charles Mingus, as she did to that of Charlie Parker ten years ago."
    ],
    "it": [
      "La sassofonista Géraldine Laurent guarda con attenzione alla musica di Charles Mingus, come dieci anni fa a quella di Charlie Parker."
    ],
    "fr": [
      "La saxophoniste Géraldine Laurent se penche sur la musique de Charles Mingus comme elle l’avait fait, voici dix ans, sur celle de Charlie Parker."
    ],
    "nl": [
      "Saxofoniste Géraldine Laurent neemt de muziek van Charles Mingus onder de loep, zoals ze tien jaar geleden deed met die van Charlie Parker."
    ],
    "es": [
      "La saxofonista Géraldine Laurent se acerca a la música de Charles Mingus, como ya hizo hace diez años a la de Charlie Parker."
    ]
  },
