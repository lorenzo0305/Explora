  "rdfs:comment": {
    "de": [
      "Die Farben der Natur: Entdecken Sie die Farben der Natur mit einer Farbjagd, einem Workshop zum Färben von Pflanzen und einem Snack in den Farben der Natur."
    ],
    "en": [
      "Nature's colors: discover the colors found in nature with a color hunt, a vegetable dyeing workshop and a snack of nature's colors."
    ],
    "it": [
      "I colori della natura: scoprite i colori presenti in natura con una caccia ai colori, un laboratorio di tintura a base vegetale e una merenda colorata."
    ],
    "fr": [
      "Les couleurs de la nature : découverte des couleurs présentes dans la nature avec une chasse aux couleurs, un atelier de teinture végétale et un goûter aux couleurs de la nature."
    ],
    "es": [
      "Los colores de la naturaleza: descubra los colores de la naturaleza con una caza del color, un taller de teñido a base de plantas y una merienda llena de color."
    ],
    "nl": [
      "Kleuren in de natuur: ontdek de kleuren in de natuur met een kleurenspeurtocht, een workshop plantaardig verven en een kleurrijke snack."
    ]
  },
