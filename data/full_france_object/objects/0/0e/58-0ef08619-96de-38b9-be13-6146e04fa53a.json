  "rdfs:comment": {
    "de": [
      "Willkommen im C?ur des Baux, hübsches Haus mit Charme und Terrasse, in ruhiger Lage im alten Dorf mit Blick auf das Château des Baux."
    ],
    "en": [
      "Welcome to Le Cœur des Baux, a pretty, charming house with terrace, in a peaceful setting in the old village with a view of the Château des Baux."
    ],
    "it": [
      "Benvenuti a Le C?ur des Baux, una graziosa casa di charme con terrazza, in un ambiente tranquillo nel vecchio villaggio con vista sullo Château des Baux."
    ],
    "fr": [
      "Bienvenue au Cœur des Baux, jolie maison de charme avec terrasse, au calme dans le vieux village avec vue sur le Château des Baux."
    ],
    "nl": [
      "Welkom bij Le C?ur des Baux, een mooi, charmant huis met terras, rustig gelegen in het oude dorp met uitzicht op het Château des Baux."
    ],
    "es": [
      "Bienvenido a Le Cœur des Baux, una bonita y encantadora casa con terraza, en un entorno tranquilo en el antiguo pueblo con vistas al Château des Baux."
    ]
  },
