  "rdfs:comment": {
    "de": [
      "Begeben Sie sich auf ein Abenteuer rund um das Schloss und entdecken Sie das Zauberwort, das die verzauberte Tür öffnet."
    ],
    "en": [
      "Go on an adventure around the castle and find the magic word that will open the enchanted door."
    ],
    "it": [
      "Avventuratevi nel castello e trovate la parola magica che aprirà la porta incantata."
    ],
    "fr": [
      "Partez à l'aventure autour du château et découvrez le mot magique qui ouvrira la porte enchantée."
    ],
    "nl": [
      "Ga op avontuur rond het kasteel en vind het magische woord dat de betoverde deur zal openen."
    ],
    "es": [
      "Embárcate en una aventura por el castillo y encuentra la palabra mágica que abrirá la puerta encantada."
    ]
  },
