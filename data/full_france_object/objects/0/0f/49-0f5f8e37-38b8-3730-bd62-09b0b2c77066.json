  "rdfs:comment": {
    "de": [
      "Nur 2 Gehminuten vom Treidelpfad der Mayenne und 10 Autominuten vom Stadtzentrum von Mayenne entfernt. Entdecken Sie das Brit Hotel Mayenne und seinen Komfort!"
    ],
    "en": [
      "2 minutes walk from the Mayenne towpath and 10 minutes drive from Mayenne city center, discover the Brit Hotel Mayenne and its comfort!"
    ],
    "it": [
      "A soli 2 minuti a piedi dall'alzaia di Mayenne e a 10 minuti in auto dal centro di Mayenne, scoprite il Brit Hotel Mayenne e il suo comfort!"
    ],
    "fr": [
      "A 2 minutes à pied du chemin de halage de la Mayenne et 10 minutes en voiture du centre ville de Mayenne, découvrez le Brit Hotel Mayenne et son confort !"
    ],
    "nl": [
      "Op 2 minuten lopen van het jaagpad van Mayenne en op 10 minuten met de auto van het centrum van Mayenne, ontdek het Brit Hotel Mayenne en zijn comfort!"
    ],
    "es": [
      "A sólo 2 minutos a pie del camino de sirga de Mayenne y a 10 minutos en coche del centro de Mayenne, descubra el Brit Hotel Mayenne y su confort"
    ]
  },
