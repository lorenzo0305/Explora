  "rdfs:comment": {
    "de": [
      "Die Observanten, Mönche aus der franziskanischen Familie, ließen sich in Ollioules nieder, wahrscheinlich unter dem Einfluss der örtlichen Herrscher, Mitte des 16."
    ],
    "en": [
      "The Observants, Franciscan monks, undoubtedly under the influence of the local lords, settled in Ollioules around the mid-16th century."
    ],
    "it": [
      "Gli Osservanti, monaci della famiglia francescana, si stabilirono a Ollioules, senza dubbio sotto l'influenza dei signori locali, intorno alla metà del XVI secolo."
    ],
    "fr": [
      "Les Observantins, moines de la famille franciscaine, s'établirent à Ollioules, sans doute sous l'influence des seigneurs locaux, vers le milieu du XVIe siècle."
    ],
    "es": [
      "Los observantes, monjes de la familia franciscana, se instalaron en Ollioules, sin duda bajo la influencia de los señores locales, hacia mediados del siglo XVI."
    ],
    "nl": [
      "De Observantijnen, monniken van de Franciscaanse familie, vestigden zich rond het midden van de 16e eeuw in Ollioules, ongetwijfeld onder invloed van de plaatselijke heren."
    ]
  },
