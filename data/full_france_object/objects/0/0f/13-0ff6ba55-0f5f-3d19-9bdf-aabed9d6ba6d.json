  "rdfs:comment": {
    "de": [
      "Restaurant und Bar. Hausgemachte Küche mit frischen, lokalen und saisonalen Produkten. Auf der mit Aromen gefüllten Karte stehen Pizzen und Tagesgerichte, die mit Liebe zubereitet werden!"
    ],
    "en": [
      "Restaurant and bar. Homemade cuisine using fresh, local and seasonal produce. A la carte, full of flavors, pizzas and daily specials cooked with love!"
    ],
    "it": [
      "Ristorante e bar. Cucina casalinga con prodotti freschi, locali e di stagione. Il menu à la carte è ricco di sapori, pizze e specialità del giorno cucinate con amore!"
    ],
    "fr": [
      "Restaurant et bar. Cuisine faite maison avec des produits frais, locaux et de saison. A la carte, remplie de saveurs, pizzas et plats du jour cuisinés avec amour !"
    ],
    "es": [
      "Restaurante y bar. Cocina casera con productos frescos, locales y de temporada. El menú a la carta está repleto de sabores, pizzas y platos del día cocinados con amor"
    ],
    "nl": [
      "Restaurant en bar. Huisgemaakte gerechten met verse, lokale en seizoensgebonden producten. Het à la carte menu zit boordevol smaken, pizza's en dagschotels die met liefde worden bereid!"
    ]
  },
