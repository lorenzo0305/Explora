  "hasDescription": [
    {
      "@id": "https://data.datatourisme.fr/bafd3faf-e0a0-3f4d-84ea-e7b3aecf070b",
      "dc:description": {
        "de": [
          "Erdgeschoss: Küche / Wohnzimmer (Fernseher), WC, Terrasse, Gartenmöbel.\n1. Stock: 1 Schlafzimmer (2 Betten 90 cm). Badezimmer (Dusche).\nKleine Haustiere erlaubt."
        ],
        "en": [
          "First floor: kitchen / living room (television), WC, terrace, garden furniture.\n1st floor : 1 bedroom (2 beds in 90). Bathroom (shower).\nSmall animals accepted."
        ],
        "it": [
          "Piano terra: cucina/soggiorno (televisione), WC, terrazza, mobili da giardino.\n1° piano: 1 camera da letto (2 letti singoli). Bagno (doccia).\nSi accettano animali di piccola taglia."
        ],
        "fr": [
          "Rez-de-chaussée : cuisine / salle de séjour (télévision), WC, terrasse, salon de jardin.\n1er étage : 1 chambre (2 lits en 90). Salle d'eau (douche).\nPetits animaux acceptés."
        ],
        "es": [
          "Planta baja: cocina / sala de estar (televisión), WC, terraza, muebles de jardín.\n1ª planta: 1 habitación (2 camas individuales). Cuarto de baño (ducha).\nSe aceptan animales pequeños."
        ],
        "nl": [
          "Begane grond: keuken/woonkamer (televisie), WC, terras, tuinmeubelen.\n1e verdieping: 1 slaapkamer (2 eenpersoonsbedden). Badkamer (douche).\nKleine dieren toegelaten."
        ]
      },
