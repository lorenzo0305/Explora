      "dc:description": {
        "de": [
          "Begleitete Wanderung mit dem VTTAE, Abfahrt in Salvignes um 17.00 Uhr. 40 Euro inklusive VVTAE-Verleih, 10 Euro (wenn ich mein eigenes VVT/VTTAE mitbringe). Informationen und Reservierungen unter 06 31 12 83 23. (Roussenn'arc)"
        ],
        "en": [
          "Guided VTTAE tour, departing from Salvignes at 5:00 pm. 40 euros VVTAE rental included, 10 euros (if I bring my own VVT/VTTAE). Information and reservations on 06 31 12 83 23. (Roussenn'arc)"
        ],
        "it": [
          "Visita guidata VTTAE, con partenza da Salvignes alle 17.00. 40 euro noleggio VVTAE incluso, 10 euro (se vengo con il mio VVT/VTTAE). Informazioni e prenotazioni allo 06 31 12 83 23. (Roussenn'arc)"
        ],
        "fr": [
          "Randonnée accompagnée en VTTAE, départ de Salvignes à 17 h 00. 40 euros la location de VVTAE comprise, 10 euros (si je viens avec mon VVT/VTTAE). Renseignements et réservations au 06 31 12 83 23. (Roussenn'arc)"
        ],
        "es": [
          "Visita guiada VTTAE, salida de Salvignes a las 17.00 h. 40 euros alquiler de VVTAE incluido, 10 euros (si vengo con mi propio VVT/VTTAE). Información y reservas en el 06 31 12 83 23. (Roussenn'arc)"
        ],
        "nl": [
          "Begeleide VTTAE-tour, vertrek vanuit Salvignes om 17.00 uur. 40 euro huur VVTAE inbegrepen, 10 euro (als ik met mijn eigen VVT/VTTAE kom). Informatie en reserveringen op 06 31 12 83 23. (Roussenn'arc)"
        ]
      },
