  "rdfs:comment": {
    "de": [
      "Philippe Faur bietet Ihnen eine große Auswahl an Eis und eisgekühlten, herzhaften und süßen Leckereien im Herzen der Stadt, auf dem Cours Mirabeau."
    ],
    "en": [
      "Philippe Faur brings you a wide variety of ice creams and sweet or savoury frozen treats in the heart of town on Cours Mirabeau."
    ],
    "it": [
      "Philippe Faur offre un'ampia varietà di gelati e prelibatezze dolci e salate nel cuore della città, sul Cours Mirabeau."
    ],
    "fr": [
      "Philippe Faur vous propose une grande variété de glaces et de gourmandises glacées, salées et sucrées en plein coeur de la ville, sur le Cours Mirabeau."
    ],
    "es": [
      "Philippe Faur ofrece una gran variedad de helados y delicias dulces y saladas en el corazón de la ciudad, en el Cours Mirabeau."
    ],
    "nl": [
      "Philippe Faur biedt een grote verscheidenheid aan ijs en zoete en hartige lekkernijen in het hart van de stad, op de Cours Mirabeau."
    ]
  },
