    ],
    "en": [
      "From green hillsides to white tuffeau"
    ],
    "it": [
      "Dalle verdi colline al bianco tufo"
    ],
    "fr": [
      "De verts coteaux au blanc tuffeau"
    ],
    "nl": [
      "Van groene heuvels tot witte tufsteen"
    ],
    "es": [
      "De las laderas verdes a la toba blanca"
    ]
  },
