    ],
    "en": [
      "Charme pasture trails"
    ],
    "it": [
      "Escursione all'alpeggio della Charme"
    ],
    "fr": [
      "Randonnée à l'Alpage de la Charme"
    ],
    "es": [
      "Senderismo por la planicie de pastos La Charme"
    ]
  },
