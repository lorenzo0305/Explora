  "rdfs:comment": {
    "de": [
      "Hochwertiges Mobilheim, neu, umkehrbare Klimaanlage, Heizung. 2 Badezimmer und 2 WCs. TV, Kaffeemaschinen, Waschmaschine und Geschirrspüler. Überdachte Terrasse + Gartentisch + Plancha."
    ],
    "en": [
      "Recent, top-of-the-range mobile home, reversible air-conditioning, heating. 2 bathrooms and 2 WCs. TV, coffee makers, washing machine + dishwasher. Covered terrace + garden table + plancha."
    ],
    "it": [
      "Recente casa mobile di alta gamma con aria condizionata e riscaldamento reversibili. 2 bagni e 2 WC. TV, caffettiera, lavatrice + lavastoviglie. Terrazza coperta + tavolo da giardino + plancha."
    ],
    "fr": [
      "Mobil-home haut de gamme, récent, clim réversible, chauffage. 2 SDB et 2 WC. TV, cafetières, lave linge + lave vaisselle. Terrasse couverte + table de jardin + plancha."
    ],
    "nl": [
      "Recente stacaravan van topklasse met omkeerbare airconditioning en verwarming. 2 badkamers en 2 toiletten. TV, koffiezetapparaat, wasmachine + vaatwasser. Overdekt terras + tuintafel + plancha."
    ],
    "es": [
      "Mobil-home reciente de gama alta con aire acondicionado y calefacción reversibles. 2 cuartos de baño y 2 WC. TV, cafetera, lavadora + lavavajillas. Terraza cubierta + mesa de jardín + plancha."
    ]
  },
