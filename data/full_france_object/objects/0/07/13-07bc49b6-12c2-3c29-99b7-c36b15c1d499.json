  "rdfs:comment": {
    "de": [
      "Nur wenige Schritte vom buddhistischen Tempel Fo Guang Shan in Bussy-Saint-Georges entfernt, lädt das Restaurant Shiang Yun Sie ein, eine geschmackvolle vegetarische Küche zu genießen!"
    ],
    "en": [
      "Vegetarian restaurant of the Buddhist temple Fo Guang Shan of Bussy-Saint-Georges."
    ],
    "it": [
      "A pochi passi dal tempio buddista Fo Guang Shan di Bussy-Saint-Georges, il ristorante Shiang Yun vi invita a gustare una cucina vegetariana ricca di sapori!"
    ],
    "fr": [
      "A quelques pas du temple bouddhiste Fo Guang Shan de Bussy-Saint-Georges, le restaurant Shiang Yun vous invite à déguster une cuisine végétarienne riche en saveur !"
    ],
    "es": [
      "Restaurante vegetariano del templo buddista Fo Guang Shan de Bussy-Saint-Georges."
    ],
