  "rdfs:comment": {
    "de": [
      "La Table du Producteur, ein Bistrobereich, empfängt Sie auf seiner Terrasse im Herzen von Saint-Cirq Lapopie, wo Sie Salate und Teller rund um die Stopfleber genießen können."
    ],
    "en": [
      "La Table du Producteur, a bistronomic space, welcomes you on its terrace in the heart of Saint-Cirq Lapopie to taste salads and plates around foie gras."
    ],
    "it": [
      "La Table du Producteur, spazio bistronomico, vi accoglie sulla sua terrazza nel cuore di Saint-Cirq Lapopie per gustare insalate e piatti a base di foie gras."
    ],
    "fr": [
      "La Table du Producteur, espace bistronomique, vous accueille sur sa terrasse au cœur de Saint-Cirq Lapopie pour déguster salades et assiettes autour du foie gras."
    ],
    "es": [
      "La Table du Producteur, un espacio bistronómico, le da la bienvenida a su terraza en el corazón de Saint-Cirq Lapopie para disfrutar de ensaladas y platos a base de foie gras."
    ],
    "nl": [
      "La Table du Producteur, een bistronomische ruimte, verwelkomt u op zijn terras in het hart van Saint-Cirq Lapopie om te genieten van salades en gerechten op basis van foie gras."
    ]
  },
