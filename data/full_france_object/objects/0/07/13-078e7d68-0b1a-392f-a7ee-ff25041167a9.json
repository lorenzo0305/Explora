  "rdfs:comment": {
    "de": [
      "Restaurant - Brasserie - Crêperie und Bar.\r\nKüche mit frischen und saisonalen Produkten (hausgemacht).\r\nAußenterrasse mit Brunnen und hundertjährigem Olivenbaum für eine ideale Essenspause."
    ],
    "en": [
      "Restaurant - brasserie - crêperie bretonne and bar.\r\nCuisine prepared with fresh seasonal produce (home-made).\r\nOutdoor terrace with fountain and century-old olive tree for an ideal lunch break."
    ],
    "it": [
      "Ristorante bretone - brasserie - crêperie e bar.\r\nCucina preparata con prodotti freschi di stagione (fatti in casa).\r\nTerrazza esterna con fontana e ulivo secolare per una pausa pranzo ideale."
    ],
    "fr": [
      "Restaurant - brasserie - crêperie bretonne et bar.\r\nCuisine élaborée avec des produits frais et de saison (fait maison).\r\nTerrasse extérieure avec fontaine et olivier centenaire pour une pause repas idéale."
    ],
    "es": [
      "Restaurante bretón - brasserie - crêperie y bar.\r\nCocina elaborada con productos frescos de temporada (caseros).\r\nTerraza exterior con fuente y olivo centenario para un almuerzo ideal."
    ],
    "nl": [
      "Bretons restaurant - brasserie - crêperie en bar.\r\nKeuken bereid met verse seizoensproducten (huisgemaakt).\r\nBuitenterras met fontein en eeuwenoude olijfboom voor een ideale lunchpauze."
    ]
  },
